{
	"redirects": [
		{
			"source": "/art/",
			"destination": "https://andrewcontreras.com"
		},
		{
			"source": "/author/:path*/",
			"destination": "https://andrewcontreras.com/about"
		},
		{
			"source": "/category/:path*/",
			"destination": "https://andrewcontreras.com"
		},
		{
			"source": "/emag/",
			"destination": "https://andrewcontreras.com"
		},
		{
			"source": "/growing-fashion/",
			"destination": "https://andrewcontreras.com/projects/growing-fashion"
		},
		{
			"source": "/inciperem/",
			"destination": "https://andrewcontreras.com/projects/inciperem"
		},
		{
			"source": "/jungle-brown-in-the-city/",
			"destination": "https://andrewcontreras.com/projects/jungle-brown-in-the-city"
		},
		{
			"source": "/libre/",
			"destination": "https://andrewcontreras.com/projects/libre"
		},
		{
			"source": "/london-postcode/",
			"destination": "https://andrewcontreras.com/projects/london-postcode"
		},
		{
			"source": "/my-city/",
			"destination": "https://andrewcontreras.com/projects/my-city"
		},
		{
			"source": "/siempre-soy/",
			"destination": "https://andrewcontreras.com/projects/siempre-soy"
		},
		{
			"source": "/tag/:path*/",
			"destination": "https://andrewcontreras.com"
		},
		{
			"source": "/trevors-lift/",
			"destination": "https://andrewcontreras.com/projects/trevors-lift"
		},
		{
			"source": "/women-and-home/",
			"destination": "https://andrewcontreras.com/projects/women-and-home"
		},
		{
			"source": "/wp-content/:path*/",
			"destination": "https://andrewcontreras.com"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "/_app/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				},
				{
					"key": "Content-Encoding",
					"value": "gzip"
				}
			]
		},
		{
			"source": "/(.*\\.(html|xml|txt))",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				},
				{
					"key": "Content-Encoding",
					"value": "gzip"
				}
			]
		}
	],
	"functions": {
		"app/api/**/*.js": {
			"maxDuration": 30
		}
	}
}
